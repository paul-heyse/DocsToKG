{
  "dashboard": {
    "title": "OntologyDownload - Database",
    "tags": ["ontology-download", "database"],
    "timezone": "browser",
    "panels": [
      {
        "title": "Query Duration",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(duckdb_query_duration_seconds_bucket[5m]))"
          }
        ],
        "type": "graph"
      },
      {
        "title": "Connection Pool Usage",
        "targets": [
          {
            "expr": "duckdb_connection_pool_active / duckdb_connection_pool_max"
          }
        ],
        "type": "gauge"
      },
      {
        "title": "Query Errors",
        "targets": [
          {
            "expr": "rate(duckdb_query_errors_total[5m])"
          }
        ],
        "type": "graph"
      },
      {
        "title": "Database Size",
        "targets": [
          {
            "expr": "duckdb_database_size_bytes"
          }
        ],
        "type": "graph"
      }
    ]
  }
}

<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/mini#"
     xml:base="http://example.org/mini"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://example.org/mini"/>
    <owl:Class rdf:about="http://example.org/mini#Root">
        <rdfs:label>Root Class</rdfs:label>
    </owl:Class>
    <owl:Class rdf:about="http://example.org/mini#Child">
        <rdfs:subClassOf rdf:resource="http://example.org/mini#Root"/>
        <rdfs:label>Child Class</rdfs:label>
    </owl:Class>
</rdf:RDF>
